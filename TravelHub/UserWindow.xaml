<Window x:Class="TravelHub.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelHub"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="800">
    <Grid>
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Width="*" Binding="{Binding idRoom}"/>
                <DataGridTextColumn Header="nameRoom" Width="*" Binding="{Binding nameRoom}"/>
                <DataGridTextColumn Header="bysyRoom" Width="*" Binding="{Binding bysyRoom}"/>
                <DataGridTextColumn Header="priceRoom" Width="*" Binding="{Binding priceRoom}"/>
                <DataGridTextColumn Header="dateTime" Width="*" Binding="{Binding dataTime}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
